#: ../src/PuzzleGames/PuzzlePeopleTable.cs:67
msgid "12th"
msgstr "12."

#: ../src/PuzzleGames/PuzzlePeopleTable.cs:71
msgid "9th"
msgstr "9."

#: ../src/PuzzleGames/PuzzlePeopleTable.cs:72
msgid "22nd"
